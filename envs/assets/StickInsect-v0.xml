    <camera name="top" mode="fixed" pos="5 0 20" xyaxes="1 0 0 0 1 0"/>
    <camera name="fixed float" mode="fixed" pos="-2 -10 6" xyaxes="1 0 0 0 0 1"/>

    <body name="torso" pos="0 0 2">
